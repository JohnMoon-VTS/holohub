{
    "application": {
        "name": "PSD pipeline simulator",
        "authors": [
            {
                "name": "John Moon <john.moon@vts-i.com>",
                "affiliation": "Valley Tech Systems, Inc."
            },
            {
                "name": "Eric Ferrara <eric.ferrara@vts-i.com>",
                "affiliation": "Valley Tech Systems, Inc."
            },
            {
                "name": "Matthew Luckenbihl <matthew.luckenbihl@vts-i.com>",
                "affiliation": "Valley Tech Systems, Inc."
            }
        ],
        "language": "C++",
        "version": "1.0.0",
        "changelog": {
            "1.0": "Initial Release"
        },
        "holoscan_sdk": {
            "minimum_required_version": "2.5.0",
            "tested_versions": [
                "2.5.0",
                "2.6.0"
            ]
        },
        "platforms": ["amd64"],
        "tags": [
            "Sensor",
            "Processing",
            "Utilities",
            "PSD",
            "FFT",
            "SDR",
            "IQ data"
        ],
        "ranking": 3,
        "dependencies": {
            "operators": [
                "file_reader",
                "fft",
                "high_rate_psd",
                "low_rate_psd",
                "vita49_psd_packetizer"
            ],
            "libraries": [{
              "name": "MatX",
              "version": "0.9.0",
              "url": "https://github.com/NVIDIA/MatX.git"
            }, {
              "name": "vrtgen",
              "version": "master",
              "url": "https://github.com/Geontech/vrtgen"
            }]
        },
        "run": {
            "command": "<holohub_app_bin>/psd_pipeline_sim",
            "workdir": "holohub_bin"
        }
    }
}
